{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/nextjs.json",
  "include": [
    ".",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "tsconfig.export.json",
    "./cli",
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  }
}
